<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC
    "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">
<liferay-portlet-app>

    <portlet>
        <portlet-name>courses-info-search</portlet-name>
        
        <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
        <friendly-url-mapping>courses-info</friendly-url-mapping>
        <friendly-url-routes>cz/cvut/portal/kos/portlet/search/routes.xml</friendly-url-routes>

        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
    </portlet>

    <portlet>
        <portlet-name>courses-info-detail</portlet-name>

        <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
        <friendly-url-mapping>courses-detail</friendly-url-mapping>
        <friendly-url-routes>cz/cvut/portal/kos/portlet/detail/routes.xml</friendly-url-routes>

        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
    </portlet>

</liferay-portlet-app>
